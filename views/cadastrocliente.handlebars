    /**  popular combo de eventos e feiras **/
    function popListFeiras(value){
        $.each(value,function(i,varfeiraevento){
            $("#cboeventos").append($('<option>',{
                value: varfeiraevento.id,
                text: varfeiraevento.nome
            }))
        })
    }

    function somenteNumeros(num) {
        var er = /[^0-9.]/;
        er.lastIndex = 0;
        var campo = num;
        if (er.test(campo.value)) {
          campo.value = "";
        }
    }

