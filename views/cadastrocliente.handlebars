    //configurando as mascaras
    function addMascarasClienteCPF(){
        $("#inpCpf").mask("999.999.999-99", {completed:function(){ 
            var validator = $( "#formCliente" ).validate();
            validator.element( "#inpCpf" );
        }});
        $("#inpTeleCliente").mask("(99) 99999-9999", {completed:function(){ 
            var validator = $( "#formCliente" ).validate();
            validator.element( "#inpTeleCliente" );
        }});
        
    }
